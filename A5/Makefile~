ifndef config
  config=debug
endif
export config

PROJECTS := A5

OPTS =  -I"irrKlang-64bit-1.5.0/include" -L"/usr/lib" irrKlang-64bit-1.5.0/bin/linux-gcc-64/libIrrKlang.so -pthread

.PHONY: all clean help $(PROJECTS)

all: $(PROJECTS)

A5: 
	@echo "==== Building A5 ($(config)) ===="
	@${MAKE} @${OPTS} --no-print-directory -C build -f Makefile

clean:
	@${MAKE} --no-print-directory -C build -f Makefile clean

